var array = [1,5,2,3,4,2,3,1,3,4];

function unique(array) {
    var res = [];
    for (var i = 0, len = array.length; i < len; i++) {
        var current = array[i];
        if (res.indexOf(current) === -1) {
            res.push(current)
        }
    }
    return res;
}

console.log(unique(array));




 array = [1,5,2,3,4,2,3,1,3,4]
 set1=new Set(array)